{
  "productName": "PhotoChooser",
  "directories": {
    "output": "release/",
    "buildResources": "build"
  },
  "files": [
    "**/*",
    "icon_win.png",
    "!*.ts",
    "!*.code-workspace",
    "!LICENSE.md",
    "!package.json",
    "!package-lock.json",
    "!src/",
    "!e2e/",
    "!hooks/",
    "!.angular-cli.json",
    "!_config.yml",
    "!karma.conf.js",
    "!tsconfig.json",
    "!tslint.json"
  ],
  "extraResources": [
    "extras"
  ],
  "win": {
    "icon": "build/icon_win.png",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "build/icon_mac.png",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "AppImage"
    ]
  }
}
